{
  "ConnectionStrings": {
    "Redis": "redis:6379"
  },
  "Adapter": {
    "BaseUrl": "https://hacker-news.firebaseio.com",
    "Bulkhead": {
      "NumberOfRequests": 1
    }
  },
  "Fetching": {
    "IsJobEnabled": true,
    "IntervalSeconds": 60,
    "CompleteUpdateMaxIntervalSeconds": 30
  },
  "Storage": {
    "SavedEntriesExpirationSeconds": 120
  }
}
