{
  "ConnectionStrings": {
    "Redis": "redis:6379"
  },
  "Adapter": {
    "BaseUrl": "https://hacker-news.firebaseio.com",
    "RateLimiting": {
      "NumberOfRequests": 50000,
      "TimePeriodSeconds": 1
    }
  },
  "Fetching": {
    "IsJobEnabled": true,
    "IntervalSeconds": 3600
  },
  "Storage": {
    "SavedEntriesExpirationSeconds": 120
  }
}
